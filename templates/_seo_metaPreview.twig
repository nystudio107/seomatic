{% endfor %}
{% else %}
<meta property="article:{{ key }}" content="{{ value |raw }}" />  <!-- {% verbatim %}{{ {% endverbatim %}seomatic.article.{{ key }}{% verbatim %} }}{% endverbatim %} -->
{% endif %}
{% endif %}
{% endfor %}
{% endif %}
{% endif %}

