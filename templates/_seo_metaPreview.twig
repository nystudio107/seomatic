{% endif %}
{% if seomaticSocial.facebookAppId %}
<meta property="fb:app_id" content="{{ seomaticSocial.facebookAppId |raw }}" /> <!-- {% verbatim %}{{ seomaticSocial.facebookAppId }}{% endverbatim %} -->
{% endif %}
{% for key, value in seomaticMeta.og %}
{% if value %}
{% if value is iterable %}
{% for subvalue in value %}
<meta property="og:{{ key }}" content="{{ subvalue |raw }}" /> <!-- {% verbatim %}{{ {% endverbatim %}seomatic.og.{{ key }}[{{ loop.index0 }}]{% verbatim %} }} {% endverbatim %}-->
{% endfor %}
{% else %}
<meta property="og:{{ key }}" content="{{ value |raw }}" /> <!-- {% verbatim %}{{ {% endverbatim %}seomatic.og.{{ key }}{% verbatim %} }}{% endverbatim %} -->
{% endif %}
{% endif %}
{% endfor %}
{% if seomaticMeta.article is defined and seomaticMeta.article %}
